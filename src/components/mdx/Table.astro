---
const { data } = Astro.props;
// data expected structure: { headers: string[], rows: string[][] }
const headers = data?.headers || [];
const rows = data?.rows || [];
---

<table>
  <thead>
    <tr>
      {headers.map((header) => (
        <th>{header}</th>
      ))}
    </tr>
  </thead>
  <tbody>
    {rows.map((row) => (
      <tr>
        {row.map((cell) => (
          <td>{cell}</td>
        ))}
      </tr>
    ))}
  </tbody>
</table>
